<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Season.
    -->
    <changeSet id="20221118233606-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="crop_id"
                                 baseTableName="season"
                                 constraintName="fk_season__crop_id"
                                 referencedColumnNames="id"
                                 referencedTableName="crops"/>

        <addForeignKeyConstraint baseColumnNames="field_id"
                                 baseTableName="season"
                                 constraintName="fk_season__field_id"
                                 referencedColumnNames="id"
                                 referencedTableName="field"/>
    </changeSet>
</databaseChangeLog>
