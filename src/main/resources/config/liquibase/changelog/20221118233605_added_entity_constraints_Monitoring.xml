<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Monitoring.
    -->
    <changeSet id="20221118233605-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="field_id"
                                 baseTableName="monitoring"
                                 constraintName="fk_monitoring__field_id"
                                 referencedColumnNames="id"
                                 referencedTableName="field"/>

        <addForeignKeyConstraint baseColumnNames="device_id"
                                 baseTableName="monitoring"
                                 constraintName="fk_monitoring__device_id"
                                 referencedColumnNames="id"
                                 referencedTableName="device"/>
    </changeSet>
</databaseChangeLog>
