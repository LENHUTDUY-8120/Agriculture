<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Device.
    -->
    <changeSet id="20221118233612-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="category_id"
                                 baseTableName="device"
                                 constraintName="fk_device__category_id"
                                 referencedColumnNames="id"
                                 referencedTableName="device_category"/>

        <addForeignKeyConstraint baseColumnNames="field_id"
                                 baseTableName="device"
                                 constraintName="fk_device__field_id"
                                 referencedColumnNames="id"
                                 referencedTableName="field"/>
    </changeSet>
</databaseChangeLog>
